{
	"name": "genanki",
	"version": "0.1.1",
	"description": "Port of python lib for generating anki decks",
	"license": "MIT",
	"repository": "jamsinclair/node-genanki",
	"author": {
		"name": "Jamie Sinclair",
		"email": "jamsinclairnz+npm@gmail.com",
		"url": "https://jamsinclair.nz"
	},
	"main": "dist",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "del dist && tsc",
		"test": "xo && ava",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"anki",
		"genanki",
		"spaced-repetition"
	],
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/node": "^13.7.4",
		"@typescript-eslint/eslint-plugin": "^2.20.0",
		"@typescript-eslint/parser": "^2.20.0",
		"ava": "^3.4.0",
		"del-cli": "^2.0.0",
		"eslint-config-xo-typescript": "^0.26.0",
		"ts-node": "^8.3.0",
		"typescript": "~3.8.2",
		"xo": "^0.26.1"
	},
	"types": "dist",
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"xo": {
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"parserOptions": {
			"project": "./tsconfig.xo.json"
		}
	}
}
